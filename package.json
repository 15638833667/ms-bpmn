{
  "name": "vue3-component-template",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "keywords": [
    "vue3",
    "vite"
  ],
  "license": "ISC",
  "author": {
    "name": "vue3-component-template",
    "email": "vue3-component-template@test.com"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "main": "./dist/vue3-component-template.umd.cjs",
  "module": "./dist/vue3-component-template.js",
  "browser": "./dist/vue3-component-template.js",
  "unpkg": "dist/vue3-component-template.iife.js",
  "exports": {
    "./dist/style.css": "./dist/style.css",
    "./css": "./dist/style.css",
    ".": {
      "import": "./dist/vue3-component-template.js",
      "require": "./dist/vue3-component-template.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "docs:dev": "vitepress dev docs --port 9000 --open",
    "docs:build": "vitepress build docs",
    "docs:deploy": "sh scripts/deploy.sh",
    "build": "run-s clean build:lib",
    "clean": "rimraf ./dist",
    "build:lib": "run-p type-check build:es build:browser",
    "type-check": "vue-tsc --noEmit",
    "build:es": "vite build",
    "build:browser": "vite -f iife build",
    "pub": "sh scripts/publish.sh",
    "push": "sh scripts/push.sh",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "core-js": "^3.36.0",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.22.0",
    "vite-plugin-dts": "^3.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.1",
    "typescript": "^5.3.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.4",
    "vitepress": "^1.0.1",
    "vue-tsc": "^2.0.6"
  },
  "description": "基于vue3 + vite的组件库模板",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/themusecatcher/vue3-component-template.git"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
